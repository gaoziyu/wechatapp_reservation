/**index.wxss**/
.container{
  background-color: #f6efe5;
}

.swiperBox{
  width: 100%;
  height: calc(100vw*900/1600);
}
.swiperImg{
  width: 100%;
}

.reservations{
  background-color: #f6efe5;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center 
}

.imgbox{
  /* height: 200rpx; */
  background-color: white;
  margin: 10rpx;
  border-radius: 20rpx;
  box-shadow: 1rpx 1rpx 5rpx 3rpx #7a7a7a;
}
.img{
  width: 350rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
}
.imgText{
  text-align: center;
  margin-bottom: 10rpx;
  font-size: small;
}